{
  "name": "semo-plugin-juejin",
  "version": "2.1.0",
  "description": "A Semo plugin to provide ability to view juejin in terminal",
  "keywords": [
    "semo",
    "plugin",
    "juejin",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/semojs/semo-plugin-juejin"
  },
  "main": "lib/index.js",
  "type": "module",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "devDependencies": {
    "@types/inquirer-autocomplete-prompt": "^3.0.3",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.15.3",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vitest/coverage-v8": "^3.1.2",
    "@vitest/eslint-plugin": "^1.1.44",
    "@vitest/ui": "^3.1.2",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.2",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1",
    "vitest": "^3.1.2"
  },
  "scripts": {
    "dev": "tsc -w",
    "build": "tsc",
    "clean": "rimraf lib",
    "test": "vitest run",
    "test:watch": "vitest ",
    "test:ui": "vitest --ui",
    "test:cov": "vitest run --coverage",
    "postversion": "git push && git push --tags",
    "prepublish": "npm run clean && npm run build",
    "prepare": "husky",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,js}\"",
    "format:check": "prettier --check \"**/*.{ts,js}\"",
    "deploy": "npm version patch",
    "deploy:minor": "npm version minor",
    "deploy:major": "npm version major"
  },
  "dependencies": {
    "@semo/core": "^2.0.10",
    "@vipzhichengfork/node-readability": "^1.0.5",
    "cheerio": "^1.0.0",
    "clipboardy": "^4.0.0",
    "dayjs": "^1.11.13",
    "fuzzy": "^0.1.3",
    "got": "^14.4.7",
    "inquirer": "9",
    "inquirer-autocomplete-prompt": "^3.0.1",
    "iterm2-version": "^5.0.0",
    "lodash": "^4.17.21",
    "marked": "^15.0.11",
    "marked-terminal": "^7.3.0",
    "shelljs": "^0.9.2",
    "terminal-image": "^3.1.1",
    "turndown": "^7.2.0",
    "turndown-plugin-gfm": "^1.0.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
